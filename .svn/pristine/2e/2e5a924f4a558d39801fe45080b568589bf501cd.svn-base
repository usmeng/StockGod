<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="2dp"
    android:orientation="vertical" >

    <RadioGroup
        android:id="@+id/myinfo_menu_layout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingBottom="-2dp"
        android:paddingTop="5dp" >

        <RadioButton
            android:id="@+id/myinfo_radio_button0"
            style="@style/main_tab_bottom"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/check_btn_bg"
            android:checked="true"
            android:paddingBottom="1dp"
            android:paddingTop="1dp"
            android:text="信息"
            android:textColor="#6a3837"
            android:textSize="15sp" />

        <RadioButton
            android:id="@+id/myinfo_radio_button1"
            style="@style/main_tab_bottom"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/check_btn_bg"
            android:paddingBottom="1dp"
            android:paddingTop="1dp"
            android:text="财富"
            android:textColor="#6a3837"
            android:textSize="15sp" />

        <RadioButton
            android:id="@+id/myinfo_radio_button2"
            style="@style/main_tab_bottom"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/check_btn_bg"
            android:paddingBottom="1dp"
            android:paddingTop="1dp"
            android:text="赛果"
            android:textColor="#6a3837"
            android:textSize="15sp" />

        <RadioButton
            android:id="@+id/myinfo_radio_button3"
            style="@style/main_tab_bottom"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/check_btn_bg"
            android:paddingBottom="1dp"
            android:paddingTop="1dp"
            android:text="设置"
            android:textColor="#6a3837"
            android:textSize="15sp" />
        
        <RadioButton
            android:id="@+id/myinfo_radio_button4"
            style="@style/main_tab_bottom"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/check_btn_bg"
            android:paddingBottom="1dp"
            android:paddingTop="1dp"
            android:text="道具"
            android:textColor="#6a3837"
            android:textSize="15sp" />
    </RadioGroup>

    <LinearLayout
        android:id="@+id/myinfo_content_layout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/main_circle_bg"
        android:orientation="vertical"
        android:padding="0dp" >
    </LinearLayout>

</LinearLayout>